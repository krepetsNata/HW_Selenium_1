<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="positive_tests">
    <test name="verifyValidData">
        <classes>
            <class name="pageTests.HardCodedTest">
                <methods>
                    <include name="verifyUserIsSuccessfullyLoggedIn"/>
                </methods>
            </class>
            <class name="pageTests.BlogTest">
                <methods>
                    <include name="verifyingLinks"/>
                </methods>
            </class>
            <class name="pageTests.SignInTest">
                <methods>
                    <include name="verifyUserSuccessfullyLogIn"/>
                    <include name="verifySpecCharsInEmail"/>
                    <include name="verifyDomainInEmail"/>
                </methods>
            </class>
            <class name="pageTests.TrainingListTest">
                <methods>
                    <include name="searchInSkills"/>
                    <include name="searchInLocations"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>